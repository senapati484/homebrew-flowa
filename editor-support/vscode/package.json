{
  "name": "flowa",
  "displayName": "Flowa",
  "description": "Syntax highlighting and file icons for Flowa programming language",
  "version": "0.1.2",
  "publisher": "senapati484",
  "homepage": "https://flowa-docs.vercel.app",
  "bugs": {
    "url": "https://github.com/senapati484/homebrew-flowa/issues"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "flowa",
    "scripting",
    "language",
    "jit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/senapati484/homebrew-flowa"
  },
  "contributes": {
    "languages": [{
      "id": "flowa",
      "aliases": ["Flowa", "flowa"],
      "extensions": [".flowa"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./icons/flowa-icon.png",
        "dark": "./icons/flowa-icon.png"
      }
    }],
    "grammars": [{
      "language": "flowa",
      "scopeName": "source.flowa",
      "path": "./syntaxes/flowa.tmLanguage.json"
    }]
  },
  "icon": "icons/flowa-icon.png"
}
